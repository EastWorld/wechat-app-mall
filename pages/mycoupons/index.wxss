.container {
    background-color:#F2f2f2;
}
.coupons-list{
    width: 100%;
    background-color: #fff;
    margin-top: 20rpx;
}

.bottom-box{
    position: absolute;
    width: 100%;
    bottom: 0;
    font-size:24rpx;
    color:#000000;
    border-top: 1rpx solid #eee;
    height:100rpx;
    line-height: 100rpx;
    background-color: #fff;
    background: url(https://cdn.it120.cc/images/weappshop/arrow-right.png) no-repeat 705rpx center;
    background-size: 15rpx auto;
}
.bottom-box .add-btn{
    margin-left: 30rpx;
    padding-left: 70rpx;
    background: url(https://cdn.it120.cc/images/weappshop/add-addr.png) no-repeat left center;
    background-size: 40rpx auto;
}
.coupons-item{
  width:630rpx;
  height: 180rpx;
  margin: 10rpx;
  padding: 50rpx;
  border-radius: 2%;
  background-color: #f1a83b;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #fff;
}
.coupons-item .moneyDiscount{
  font-size: 48rpx;
}
.coupons-item .itemUseMin{
  font-size: 24rpx;
}
.coupons-item .itemName{
  font-size: 24rpx;
}
.coupons-item .itemValid{
  font-size: 24rpx;
}